{
  "version": 2,
  "name": "preco-parser",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/app.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "dist/app.js" },
    {
      "src": "/static",
      "dest": "static/*"
    }
  ],
  "env": {
    "ADMIN_LOGIN": "QWE",
    "ADMIN_PASSWORD": "QWE"
  }
}
